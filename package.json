{
  "name": "trickcal-team-manager",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "predev": "node scripts/generate-version.mjs",
    "dev": "vite",
    "prebuild": "node scripts/generate-version.mjs",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "vitest",
    "prepare": "husky",
    "lint-staged": "lint-staged",
    "lint": "biome lint --write .",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "data:export": "node scripts/sync-apostles.mjs export",
    "data:import": "node scripts/sync-apostles.mjs import"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,json,css,scss,html}": [
      "biome check --write ."
    ]
  },
  "dependencies": {
    "@ssgoi/react": "^4.2.1",
    "@supabase/supabase-js": "^2.97.0",
    "@tailwindcss/vite": "^4.2.0",
    "es-hangul": "^2.3.8",
    "html-to-image": "^1.11.13",
    "lodash": "^4.17.23",
    "lz-string": "^1.5.0",
    "react": "^19.2.4",
    "react-dom": "19.2.4",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.13.0",
    "shepherd.js": "^14.5.1",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@biomejs/biome": "2.4.4",
    "@tailwindcss/postcss": "^4.2.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/lodash": "^4.17.23",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.3",
    "daisyui": "^5.5.19",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.2.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "^1.2.0",
    "vite-tsconfig-paths": "^6.1.1",
    "vitest": "^4.0.18"
  }
}
